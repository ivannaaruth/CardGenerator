{
    "name": "pbpu/cardproject",
    "description": "a card project",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "fika",
            "email": "faidatul2205@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {},
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "CardMaker\\": "CardItemMaker/packages/src/"
        }
    },
    "scripts": {
        "post-package-install": [
            "php artisan vendor:publish --tag=card --ansi --force"
        ],
        "post-package-update": [
            "php artisan vendor:publish --tag=card --ansi --force"
        ],
        "post-update-cmd": [
            "php artisan vendor:publish --provider=vendor/CardItemMaker"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [],
            "providers": [
                "CardMaker\\CardServiceProvider"
            ]
        }
    }
}
